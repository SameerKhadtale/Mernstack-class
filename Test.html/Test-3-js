// Question check pallindrome or not

var string = "madam"; 
function checkP(str){
    if (str.length < 2){
        return "str not valid"
    }
        for (var i=0; i < str.length/2; i++){
            if (str[i] !== str[str.length-1-i]){
                return false
            }
            
        }
        return true

}
console.log(checkP(string))